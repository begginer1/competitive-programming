#include<stdio.h>
int main()
{
    int i,j,n,t;
    scanf("%d",&t);
    while(t)
    {scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=(n*2);j++)
        {if(j>=i+1&&j<=(n*2)-i)
        printf("#");
        else
        printf("*");
    }
    printf("\n");
        
    }
    t--;   
    }
    return 0;
}
