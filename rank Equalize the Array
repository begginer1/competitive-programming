#include<stdio.h>
void fun(int a[],int size)
{
int i,j,temp=0;
for(i=0;i<size-1;i++)
{
for(j=i+1;j<size;j++)
{
if(a[i]>a[j])
{
temp=a[i];
a[i]=a[j];
a[j]=temp;
} 
}
}
}

int main()
{
int i,k,size,c1=1,c2=1;
scanf("%d",&size);
int a[size];
for(i=0;i<size;i++)
scanf("%d",&a[i]);
fun(a,size);
for(i=0;i<size-1;i++)
{
if(a[i]==a[i+1])
{
    c1++;
}
else
if(c1>c2)
{
c2=c1;
c1=1;
}
else 
c1=1;
}
if(c1==1&&c2==1)
printf("%d ",size-1);
else
{
k=(c1>c2)?c1:c2;
if(size-k<size-(size-k))
printf("%d",size-k);
else
printf("%d",size-(size-k));
}
return 0;
}
