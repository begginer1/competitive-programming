#include<stdio.h>
void fun(int a[],int size)
{
int i,j,temp;
for(i=0;i<size-1;i++)
{
for(j=i+1;j<size;j++)
{
if(a[i]>a[j])
{
temp=a[j];
a[j]=a[i];
a[i]=temp;
} 
}
}
}

int main()
{
int i,size,c1=1,c2=1;
scanf("%d",&size);
int a[size];
for(i=0;i<size;i++)
scanf("%d",&a[i]);
fun(a,size);
for(i=0;i<size-1;i++)
{
if(a[i]==a[i+1])
c1++;
else
if(c1>c2)
{
c2=c1;
c1=0;
}
}
if(c1==1&&c2==1)
printf("%d ",size-1);
else
{
if(c1>c2)
printf("%d",size-c1);
else
printf("%d",size-c2);
}
return 0;
}
